
## ========================== input: now is just placeholder =============================================
layer{
 	name: "image"
  	type: "Input"
  	top: "image"
  	input_param{ 
  		shape: { dim: 1 dim: 3 dim: 256 dim: 256 } 
  	}
}
layer{
	name: "gtMaps"
	type: "Input"
	top: "gtMaps"
	input_param{
		shape: { dim: 1 dim: 16 dim: 64 dim: 64 } 
		## Ground Truth Map: batchSize x nStack x outDim x 64 x 64
	}
}


## =========================== preprocessing ====================================== ##

layer {
	name: "conv1_prepro"
	type: "Convolution"
	bottom: "image" 			# 256x256
	top: "conv1_prepro" 		# 128x128
	# learning rate and decay multipliers for the filters
    param { lr_mult: 1 decay_mult: 1 }
    # learning rate and decay multipliers for the biases
    param { lr_mult: 2 decay_mult: 0 }
    convolution_param{
    	num_output: 64
    	kernel_size: 6
    	stride: 2
    	pad: 2
    	weight_filler{
    		type: "gaussian"
    		std: 0.01
    	}
    	bias_filler{
    		type: "constant"
    		value: 0
    	}
    }
}
layer{
	name: "bn1_prepro"
	type: "BatchNorm"
	bottom: "conv1_prepro"
	top: "bn1_prepro"
	batch_norm_param{
		use_global_stats: true
		moving_average_fraction: 0.9
		eps: 1e-5
	}
}

