  name: "newP3pool"
  type: "Pooling"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 2
  }
}

layer {
  bottom: "P3"
  top: "upP3"
  name: "upP3"
  type: "Deconvolution"
  convolution_param {
    kernel_h : 2
    kernel_w : 2
    stride_h: 2
    stride_w: 2
    pad_h: 0
    pad_w: 0
    num_output: 256
    group: 256
    bias_term: false
    weight_filler {
      type: "bilinear"
    }
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
}

layer {
  bottom: "conv2_2"
  top: "newC2"
  name: "newC2"
  type: "Convolution"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  convolution_param {
    num_output: 256
    kernel_size: 1
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 0.5 }
  }
}

layer {
  bottom: "upP3"
  bottom: "newC2"
  top: "upP3crop"
  name: "upP3crop"
  type: "Crop"
  crop_param {
    axis: 2
    offset: 0
  }
}

layer {
  bottom: "upP3crop"
  bottom: "newC2"
  top: "P2"
  name: "P2"
  type: "Eltwise"
  eltwise_param {
    operation: SUM
  }
}

layer {
  bottom: "P2"
  top: "newP2"
  name: "newP2"
  type: "Convolution"
  param { lr_mult: 1.0 }
  param { lr_mult: 2.0 }
  convolution_param {
    num_output: 256
    pad: 1
    kernel_size: 3
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 0.5 }
  }
}

layer {
  bottom: "newP2"
  top: "newP2pool"
  name: "newP2pool"
  type: "Pooling"
  pooling_param {
    pool: MAX
    kernel_size: 4
    stride: 4
  }
}

layer {
  bottom: "newC4"
  bottom: "newP3pool"
  bottom: "newP2pool"
  top: "all_pyra"
